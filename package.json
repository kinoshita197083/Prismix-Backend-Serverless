  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "serverless offline",
    "build": "serverless package",
    "start": "nodemon --exec serverless offline start",
    "info": "sls info",
    "deployments": "serverless deploy list",
    "prisma:generate:dev": "npx dotenv -e .env.dev -- prisma generate",
    "prisma:db:push:dev": "npx dotenv -e .env.dev -- prisma db push",
    "prisma:db:pull:dev": "npx dotenv -e .env.dev -- prisma db pull",
    "build:schema:dev": "npx dotenv -e .env.dev prisma validate",
    "prisma:studio:dev": "npx dotenv -e .env.dev prisma studio",
    "test:enqueueImage": "serverless invoke local --function enqueueImage --path src/test-events/s3-event.json",
    "test:worker": "serverless invoke local --function worker --path src/test-events/sqs-event.json"
  },
